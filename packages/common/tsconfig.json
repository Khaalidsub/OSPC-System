{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "rootDir": "./src",
    "incremental": true,
"paths": {
  "interfaces/*":["./src/interfaces/*"],
  "errors/*":["./src/errors/*"],
  "enums/*":["./src/enums/*"]
}
  },

  
  "include": ["./src/**/*.ts"]
}
