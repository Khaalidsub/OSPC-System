{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "components/*":["./components/*"],
      "layouts/*":["./layouts/*"],
      "types/*":["./types/*"],
      "lib/*":["./lib/*"],
      "utililites/*":["./utilities/*"],
      "styles/*":["./styles/*"],
      // "public/*":["./public/*"],  
      // "@common/*":[
      //   "./node_modules/@ospc/common/src/*"
      //  ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "next.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}
